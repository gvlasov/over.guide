    "class-validator": "^0.12.2",
    "cls-hooked": "^4.2.2",
    "crypto-js": "^4.0.0",
    "fast-shuffle": "^4.2.2",
    "lodash.difference": "^4.5.0",
    "lodash.flatten": "^4.4.0",
