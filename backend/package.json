    "mysql2": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "seedrandom": "^3.0.5",
    "sequelize": "~5.22.2",
    "sequelize-typescript": "^1.1.0",
    "umzug": "^2.3.0",
    "ytdl-core": "^4.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
